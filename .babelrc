{
  "presets": [
    [
      "env", {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "node": "8"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread"
  ],

  "env": {
    "test": {
      "presets": [
        [
          "env", {
            "modules": "commonjs",
            "useBuiltIns": true,
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}